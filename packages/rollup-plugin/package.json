{
  "name": "rollup-plugin-jsshaker",
  "version": "0.2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/kermanx/jsshaker"
  },
  "keywords": [
    "rollup-plugin",
    "vite-plugin",
    "jsshaker"
  ],
  "exports": {
