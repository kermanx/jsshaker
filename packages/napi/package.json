{
  "name": "jsshaker",
  "version": "0.0.1",
  "type": "commonjs",
  "main": "index.js",
  "browser": "browser.js",
  "napi": {
    "binaryName": "jsshaker",
    "packageName": "@jsshaker/binding",
    "targets": [
      "x86_64-unknown-linux-gnu",
      "x86_64-pc-windows-msvc",
      "x86_64-apple-darwin",
      "aarch64-apple-darwin",
      "wasm32-wasip1-threads"
    ],
    "wasm": {
      "browser": {
        "fs": false,
        "asyncInit": true
      }
    }
  },
  "bin": {
    "jsshaker": "./cli.js"
  },
  "files": [
    "index.d.ts",
    "index.js",
    "cli.js"
  ],
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^3.1.5",
    "@napi-rs/wasm-runtime": "^1.0.3",
    "emnapi": "^1.5.0",
    "ohash": "^2.0.11"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "build:wasi": "pnpm build:debug --target wasm32-wasip1-threads",
    "prepublishOnly": "napi prepublish -t npm",
    "universal": "napi universal",
    "version": "napi version"
  },
  "packageManager": "pnpm@10.15.1",
  "optionalDependencies": {
    "@jsshaker/binding-linux-x64-gnu": "0.0.0",
    "@jsshaker/binding-win32-x64-msvc": "0.0.0",
    "@jsshaker/binding-darwin-x64": "0.0.0",
    "@jsshaker/binding-darwin-arm64": "0.0.0",
    "@jsshaker/binding-wasm32-wasi": "0.0.0"
  }
}