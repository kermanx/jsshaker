{
  "type": "module",
  "scripts": {
    "e2e-build:commonjs": "tsup --config ./commonjs/tsup.config.js",
    "e2e-build:vue": "vite build ./vue",
    "e2e-build:vuetify": "vite build ./vuetify",
    "e2e-build:react": "vite build ./react",
    "e2e-build-shaken": "cross-env DISABLE_TREE_SHAKE=0 pnpm run --parallel \"/^e2e-build:.*/\"",
    "e2e-build-unshaken": "cross-env DISABLE_TREE_SHAKE=1 pnpm run --parallel \"/^e2e-build:.*/\"",
    "build": "pnpm run --parallel \"/^e2e-build-.*/\"",
    "test": "vitest"
  },
  "devDependencies": {
    "jsshaker": "workspace:*",
    "@types/node": "^22.7.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/browser": "^2.1.3",
    "antd": "^5.22.2",
    "cross-env": "^7.0.3",
    "picocolors": "^1.1.1",
    "playwright": "^1.48.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vitest": "^2.1.3",
    "vue": "^3.5.12",
    "vuetify": "^3.7.3"
  },
  "packageManager": "pnpm@9.11.0"
}
